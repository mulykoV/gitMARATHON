@model gitMARATHON.Models.Score

@{
    ViewData["Title"] = "Delete Score";
}

<h2>Are you sure you want to delete the score for lecture @Model.LectureId?</h2>

<form method="post">
    <button type="submit" class="btn btn-danger">Delete</button>
    <a href="@Url.Action("Index", "Score")" class="btn btn-secondary">Cancel</a>
</form>
